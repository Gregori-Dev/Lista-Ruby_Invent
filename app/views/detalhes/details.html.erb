<html lang="en">
<head>
  <title>Detalhes</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/js/bootstrap.bundle.min.js"></script>
</head>
<style>
body {
  background-color: rgba(67, 112, 112);
}
</style>
<body>

<nav class="navbar navbar-expand-sm bg-dark navbar-dark">
  <div class="container-fluid">
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link active" href="http://127.0.0.1:3000/inicio">Inicio</a>
      </li>   
    </ul>
  </div>
</nav>

<div class="container">
  <div class="row">
    <div class="col-sm bg-light text-dark p-0">   
     <p class= "bg-warning text-dark p-0">Nome</p>
     <div></div>
     <label><%=@nomeArray[0]%></label>
    </div>
     <div class="col-sm bg-light text-dark p-0">
      <p class= "bg-warning text-dark p-0">Idade</p>
      <div></div>
     <label><%=@idadeArray[0]%></label>
    </div>
    <div class="col-sm bg-light text-dark p-0">
      <p class= "bg-warning text-dark p-0">CPF</p>
      <div></div>
     <label><%=@cpfArray[0]%></label>
    </div>
  </div>
  <div class="row">

  </div>
  <div class="row">
    <div class="col-sm bg-light text-dark p-0">
    <p class= "bg-warning text-dark p-0">CEP</p>
    <div></div>
     <label><%=@cepArray[0]%></label>    
     </div>
    <div class="col-sm bg-light text-dark p-0">
      <p class= "bg-warning text-dark p-0">Email</p>
      <div></div>
     <label><%=@emailArray[0]%></label>
    </div>
    <div class="col-sm bg-light text-dark p-0">
    <p class= "bg-warning text-dark p-0">Data de nascimento</p>
     <label><%=@dataDeNascimentoArray[0]%></label>
    </div>
  </div>
</div>
 <div class="col-lg-11" style="text-align: right;">
  <button type="button" class="btn btn-success">Aplicar</button>
  <button type="button" class="btn btn-danger">Cancelar</button>
</div> 

</body>
</html>